{
  "name": "aws-sql-migration",
  "version": "0.0.1",
  "description": "Serverless RDS MySQL migration function using Typescript",
  "private": true,
  "scripts": {
    "build-ts": "rm -Rf node_modules && npm install && rm -Rf dist && tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest",
    "package": "sls package",
    "deploy": "sls deploy",
    "deploy-attach-s3-bucket": "sls s3deploy",
    "deploy-local": "sls deploy --aws-profile=una-reporting",
    "deploy-attach-s3-bucket-from-local": "sls s3deploy --aws-profile=una-reporting",
    "package-cdk": "rm -Rf deployment && yarn tsc -p . --outDir deployment && copyfiles -f src/migrations/*.sql  deployment/migrations && yarn install --prod --modules-folder deployment/node_modules"
  },
  "dependencies": {
    "mysql": "^2.17.1",
    "semver": "^6.3.0",
    "serverless": "^1.52.0",
    "serverless-mysql": "^1.3.0",
    "source-map-support": "^0.5.10",
    "umzug-typescript-helper": "^1.0.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.12.18",
    "@types/semver": "^6.0.2",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.474.0",
    "jest": "^24.7.1",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "copy-webpack-plugin": "^5.0.4",
    "copyfiles": "^2.1.1"
  }
}
